{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "installCommand": "npx @puppeteer/browsers install chrome@116.0.5793.0"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "crons": [
        {
            "path": "/scrape",
            "schedule": "0 11 * * *"
        }
    ]
}