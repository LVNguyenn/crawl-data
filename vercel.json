{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            },
            "build": {
                "env": {
                    "NODE_ENV": "production"
                },
                "npmScript": "install-puppeteer"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "crons": [
        {
            "path": "/scrape",
            "schedule": "0 11 * * *"
        }
    ]
}